{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "mvp/schemas/compatible_set.schema.json",
  "title": "BASURIN canonical compatible set",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_version",
    "event_id",
    "compatible_geometry_ids"
  ],
  "properties": {
    "schema_version": {
      "type": "integer",
      "const": 1
    },
    "event_id": {
      "type": "string",
      "minLength": 1
    },
    "compatible_geometry_ids": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    }
  }
}
